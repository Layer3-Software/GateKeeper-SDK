{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": "^18.0.0"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "eslint '{,!(node_modules)/**/}*.{ts,tsx}' --max-warnings=0",
    "commit": "git-cz",
    "prettier:fix": "prettier --write '{,!(node_modules)/**/}*.{ts,tsx}'  --config ./.prettierrc",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "semantic-release": "semantic-release --branches main"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "name": "@layer3/gatekeeper-sdk",
  "author": "Nico Limo",
  "module": "dist/gatekeeper-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/gatekeeper-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gatekeeper-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-image": "^2.1.1",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/addons": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/react-webpack5": "^7.0.12",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.13",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.4.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rollup-plugin-image-files": "^1.4.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-static-files": "^0.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.5",
    "size-limit": "^8.1.0",
    "storybook": "^7.0.12",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Layer3-Software/Gatekeeper-SDK.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
